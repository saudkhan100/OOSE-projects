<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.tts4.StudentLoginController">
    <Label text="Student Login" />
    <TextField fx:id="studentIdField" promptText="Student ID" />
    <PasswordField fx:id="passwordField" promptText="Password" />
    <Button text="Login" onAction="#loginButtonClicked" />
</VBox>

